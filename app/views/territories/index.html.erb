<h1>Territory list</h1>


<%= link_to "Create New Territory", new_territory_path %>

<% @states.each do |state|  %>
  <h3><%= state.name.camelcase %></h3>
  <%= link_to "Edit", edit_territory_path(state) %>
  <%= link_to "Delete", territory_path(state), data: { turbo_method: :delete,  turbo_confirm: 'Are you sure?' } %>
<% end %>

